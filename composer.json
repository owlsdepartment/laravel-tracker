{
    "name": "owlsdepartment/laravel-tracker",
    "description": "Laravel Tracking Package",
    "keywords": [
        "laravel",
        "tracking",
        "aggregation",
        "analytics"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Komarnicki",
            "email": "mateusz.komarnicki@owlsdepartment.com"
        },
        {
            "name": "Micha≈Ç Szlagor",
            "email": "michal.szlagor@owlsdepartment.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "OwlsDepartment\\LaravelTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gloudemans\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
